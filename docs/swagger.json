{
    "swagger": "2.0",
    "info": {
        "description": "This is a price comparator application.",
        "title": "Comparator API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
